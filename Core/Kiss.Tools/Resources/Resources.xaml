<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:rs="clr-namespace:Kiss.Tools.Resources"
                    xmlns:cv="clr-namespace:Kiss.Tools.Converters">

    <cv:TrueToVisibleConverter x:Key="{x:Static rs:Converters.TrueToVisibleConverter}"/>
    <cv:TrueToHiddenConverter x:Key="{x:Static rs:Converters.TrueToHiddenConverter}"/>
    <cv:TrueToCollapsedConverter x:Key="{x:Static rs:Converters.TrueToCollapsedConverter}"/>
    <cv:InverseBooleanConverter x:Key="{x:Static rs:Converters.InverseBooleanConverter}"/>

</ResourceDictionary>