<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:atr="clr-namespace:AiSoft.Tools.Resources"
                    xmlns:atc="clr-namespace:AiSoft.Tools.Converters">

    <atc:TrueToVisibleConverter x:Key="{x:Static atr:Converters.TrueToVisibleConverter}"/>
    <atc:TrueToHiddenConverter x:Key="{x:Static atr:Converters.TrueToHiddenConverter}"/>
    <atc:TrueToCollapsedConverter x:Key="{x:Static atr:Converters.TrueToCollapsedConverter}"/>
    <atc:EnumToDescriptionConverter x:Key="{x:Static atr:Converters.EnumToDescriptionConverter}"/>
    <atc:InverseBooleanConverter x:Key="{x:Static atr:Converters.InverseBooleanConverter}"/>
    <atc:PrecisionConverter x:Key="{x:Static atr:Converters.PrecisionConverter}"/>
    <atc:PrecisionNumberConverter x:Key="{x:Static atr:Converters.PrecisionNumberConverter}"/>

</ResourceDictionary>